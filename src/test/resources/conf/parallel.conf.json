{
  "server": "hub-cloud.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",
  "capabilities": {
    "browserstack.use_w3c": true,
    "bstack:options": {
      "sessionName": "parallel_test",
      "buildName": "serenity-browserstack",
      "projectName": "Test App",
      "source": "serenity:sample-selenium-4-v1.0",
      "debug": true
    }
  },
  "environments": {
    "chrome": {
      "browser": "chrome"
    },
    "firefox": {
      "browser": "firefox"
    },
    "safari": {
      "browser": "safari",
      "bstack:options": {
        "os": "OS X",
        "osVersion": "High Sierra"
      }
    },
    "ie": {
      "browser": "Internet Explorer",
      "browserVersion": "10",
      "bstack:options": {
        "os": "Windows",
        "seleniumVersion": "3.12.0"
      }
    }
  }
}
